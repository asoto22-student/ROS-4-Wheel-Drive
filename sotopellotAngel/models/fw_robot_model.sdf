            </limit>
        </joint>

        <!-- Back Right Wheel -->
        <link name='b_right_wheel'>
            <pose>-0.15 -0.13 0.1 0 1.5707 1.5707</pose>
            <collision name='brw_collision'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>

                <surface>
                    <friction>
                        <ode>
                            <mu>1.0</mu>
                            <mu2>1.0</mu2>
                            <slip1>0.0</slip1>
                            <slip2>0.0</slip2>
                        </ode>
                    </friction>
                </surface>
                
            </collision>
            <visual name='brw_visual'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
                <material></material>
            </visual>
        </link>
        <joint type='revolute' name='b_right_wheel_hinge'>
            <pose> 0 0 0.03 0 0 0 </pose>
            <child>b_right_wheel</child>
            <parent>chassis</parent>
            <axis>
                <xyz>0 0 1</xyz>
            </axis>
            <dynamics>
                <friction>100.0</friction>
                <spring_stiffness>0.1</spring_stiffness>
            </dynamics>
            <limit>
                <effort>0.01</effort>
                <velocity>0.005</velocity>
            </limit>
        </joint>
    </model>
</sdf>