<?xml version='1.0'?>
<sdf version="1.7">
    <model name='fw_robot'>
        <!-- Robot Chassis -->
        <link name='chassis'>
            <pose>0 0 0.1 0 0 0</pose>
            <static>true</static>
            <inertial>
                <mass>25</mass>
            </inertial>

            <collision name='collision'>
                <geometry>
                    <box>
                        <size> 0.45 0.2 0.1 </size>
                    </box>
                </geometry>
            </collision>

            <visual name='visual'>
                <geometry>
                    <box>
                        <size> 0.45 0.2 0.1 </size>
                    </box>
                </geometry>
                <material>
                    <ambient>0.1 0.1 0.1 1</ambient>
                    <diffuse>0.1 0.1 0.2 1</diffuse>
                    <specular>0 0 0 0</specular>
                    <emissive>0 0 0 1</emissive>
                </material>
            </visual>
        </link>
        
        <!-- Front Left Wheel -->
        <link name='f_left_wheel'>
            <pose>0.15 0.13 0.1 0 1.5707 1.5707</pose>
            <collision name='flw_collision'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                        <slip1>0.0</slip1>
                        <slip2>0.0</slip2>
                    </ode>
                </friction>
            </collision>
            <visual name='flw_visual'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
                <material></material>
            </visual>
        </link>
        <joint type='revolute' name='f_left_wheel_hinge'>
            <pose> 0 0 -0.03 0 0 0 </pose>
            <child>f_left_wheel</child>
            <parent>chassis</parent>
            <axis>
                <xyz>0 0 1</xyz>
            </axis>
            <dynamics>
                <friction>100.0</friction>
                <spring_stiffness>0.1</spring_stiffness>
            </dynamics>
            <limit>
                <effort>0.01</effort>
                <velocity>0.005</velocity>
            </limit>
        </joint>

        <!-- Front Right Wheel -->
        <link name='f_right_wheel'>
            <pose>0.15 -0.13 0.1 0 1.5707 1.5707</pose>
            <collision name='frw_collision'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>

                <surface>
                    <friction>
                        <ode>
                            <mu>1.0</mu>
                            <mu2>1.0</mu2>
                            <slip1>0.0</slip1>
                            <slip2>0.0</slip2>
                        </ode>
                    </friction>
                </surface>
                
            </collision>
            <visual name='frw_visual'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
                <material></material>
            </visual>
        </link>
        <joint type='revolute' name='f_right_wheel_hinge'>
            <pose> 0 0 0.03 0 0 0 </pose>
            <child>f_right_wheel</child>
            <parent>chassis</parent>
            <axis>
                <xyz>0 0 1</xyz>
            </axis>
            <dynamics>
                <friction>100.0</friction>
                <spring_stiffness>0.1</spring_stiffness>
            </dynamics>
            <limit>
                <effort>0.01</effort>
                <velocity>0.005</velocity>
            </limit>
        </joint>

        <!-- Back Left Wheel -->
        <link name='b_left_wheel'>
            <pose>-0.15 0.13 0.1 0 1.5707 1.5707</pose>
            <collision name='blw_collision'>
